<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <!--
    init-method = 初始化方法吗
    destroy-method = 销毁方法吗
    -->
    <bean id="JavaBean" class="com.ioc_04.JavaBean" init-method="init" destroy-method="clear"/>

    <!--单例模式-->
<!--    <bean id="JavaBean1" class="com.ioc_04.JavaBean1"/>-->

    <!--多列-->
    <bean id="JavaBean1" class="com.ioc_04.JavaBean1" scope="prototype"/>

</beans>